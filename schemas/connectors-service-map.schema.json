{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$title": "ConnectorsServiceMap",
  "$ref": "#/definitions/ConnectorsServiceMap",
  "definitions": {
    "ConnectorsServiceMap": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "pattern": "^[0-9A-HJKMNP-TV-Z]{26}$"
        },
        "brand": {
          "type": "string",
          "const": "brAInwav"
        },
        "generatedAt": {
          "type": "string",
          "format": "date-time"
        },
        "ttlSeconds": {
          "type": "integer",
          "exclusiveMinimum": 0
        },
        "connectors": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "minLength": 1
              },
              "displayName": {
                "type": "string",
                "minLength": 1
              },
              "version": {
                "type": "string",
                "minLength": 1
              },
              "endpoint": {
                "type": "string",
                "format": "uri"
              },
              "auth": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string",
                    "enum": [
                      "apiKey",
                      "bearer",
                      "none"
                    ]
                  },
                  "headerName": {
                    "type": "string",
                    "minLength": 1
                  },
                  "queryParam": {
                    "type": "string",
                    "minLength": 1
                  }
                },
                "required": [
                  "type"
                ],
                "additionalProperties": false
              },
              "scopes": {
                "type": "array",
                "items": {
                  "type": "string",
                  "minLength": 1
                },
                "minItems": 1
              },
              "quotas": {
                "type": "object",
                "properties": {
                  "perMinute": {
                    "type": "integer",
                    "exclusiveMinimum": 0
                  },
                  "perHour": {
                    "type": "integer",
                    "exclusiveMinimum": 0
                  },
                  "perDay": {
                    "type": "integer",
                    "exclusiveMinimum": 0
                  }
                },
                "required": [
                  "perMinute",
                  "perHour"
                ],
                "additionalProperties": false
              },
              "enabled": {
                "type": "boolean",
                "default": true
              },
              "metadata": {
                "type": "object",
                "properties": {
                  "brand": {
                    "type": "string",
                    "const": "brAInwav"
                  }
                },
                "required": [
                  "brand"
                ],
                "additionalProperties": {},
                "default": {
                  "brand": "brAInwav"
                }
              },
              "tags": {
                "type": "array",
                "items": {
                  "type": "string",
                  "minLength": 1
                },
                "minItems": 1
              },
              "timeouts": {
                "type": "object",
                "properties": {
                  "requestMs": {
                    "type": "integer",
                    "exclusiveMinimum": 0
                  },
                  "connectMs": {
                    "type": "integer",
                    "exclusiveMinimum": 0
                  }
                },
                "additionalProperties": false
              },
              "availability": {
                "type": "object",
                "properties": {
                  "status": {
                    "type": "string",
                    "enum": [
                      "online",
                      "offline",
                      "degraded",
                      "unknown"
                    ],
                    "default": "unknown"
                  },
                  "lastCheckedAt": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "lastSuccessAt": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "lastFailureAt": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "failureReason": {
                    "type": "string"
                  }
                },
                "additionalProperties": false
              },
              "status": {
                "type": "string",
                "enum": [
                  "online",
                  "offline",
                  "degraded",
                  "unknown"
                ],
                "default": "unknown"
              },
              "ttlSeconds": {
                "type": "integer",
                "exclusiveMinimum": 0
              },
              "expiresAt": {
                "type": "string",
                "format": "date-time"
              },
              "telemetry": {
                "type": "object",
                "properties": {
                  "latencyP95Ms": {
                    "type": "number",
                    "minimum": 0
                  },
                  "requestCount": {
                    "type": "number",
                    "minimum": 0
                  },
                  "errorCount": {
                    "type": "number",
                    "minimum": 0
                  }
                },
                "additionalProperties": false
              }
            },
            "required": [
              "id",
              "displayName",
              "version",
              "endpoint",
              "auth",
              "scopes",
              "quotas",
              "ttlSeconds",
              "expiresAt"
            ],
            "additionalProperties": false
          }
        },
        "signature": {
          "type": "string",
          "minLength": 1
        },
        "metadata": {
          "type": "object",
          "additionalProperties": {}
        }
      },
      "required": [
        "id",
        "brand",
        "generatedAt",
        "ttlSeconds",
        "connectors",
        "signature"
      ],
      "additionalProperties": false
    }
  }
}