      "types": "./src/envelope.ts"
    },
    "./evidence": {
      "import": "./dist/evidence.js",
      "types": "./src/evidence.ts"
    },
    "./events": {
      "import": "./dist/events.js",
      "types": "./src/events.ts"
    },
    "./commands": {
      "import": "./dist/commands.js",
      "types": "./src/commands.ts"
    },
    "./trace-context": {
      "import": "./dist/trace-context.js",
      "types": "./src/trace-context.ts"
    },
    "./trace-context.js": {
      "import": "./dist/trace-context.js",
      "types": "./src/trace-context.ts"
    },
    "./agents": {
      "import": "./dist/agents.js",
      "types": "./src/agents.ts"
    },
    "./schema-registry-types": {
      "import": "./dist/schema-registry-types.js",
      "types": "./src/schema-registry-types.ts"
    },
    "./topic-acl": {
      "import": "./dist/topic-acl.js",
      "types": "./src/topic-acl.ts"
    },
    "./topic-acl.js": {
      "import": "./dist/topic-acl.js",
      "types": "./src/topic-acl.ts"
    },
    "./outbox-types": {
      "import": "./dist/outbox-types.js",
