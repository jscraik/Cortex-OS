  "scripts": {
    "build": "tsup src/**/*.ts --dts --format esm",
    "test": "node ../../../scripts/vitest-safe.mjs run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:safe": "node ../../../scripts/vitest-safe.mjs run"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/js-yaml": "^4.0.9",
    "@types/micromatch": "^4.0.9",
    "eventsource": "^4.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "tsup": "8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
