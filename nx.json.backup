{
  "extends": "nx/presets/npm.json",
  "namedInputs": {
    "sharedGlobals": [
      "{workspaceRoot}/tsconfig.base.json",
      "{workspaceRoot}/package.json",
      "{workspaceRoot}/nx.json",
      "{workspaceRoot}/.eslintrc.json",
      "{workspaceRoot}/biome.json",
      "{workspaceRoot}/pnpm-lock.yaml",
      "{workspaceRoot}/eslint.config.js"
    ],
    "default": [
      "{projectRoot}/**/*",
      "!{projectRoot}/dist/**",
      "!{projectRoot}/coverage/**",
      "!{projectRoot}/.nx/cache/**",
      "sharedGlobals"
    ],
    "production": [
      "default",
      "!{projectRoot}/**/*.spec.ts",
      "!{projectRoot}/**/*.test.ts",
      "!{projectRoot}/**/__tests__/**",
      "!{projectRoot}/**/*.spec.tsx",
      "!{projectRoot}/**/*.test.tsx"
    ],
    "test": [
      "default"
    ],
    "lint": [
      "default"
    ],
    "typecheck": [
      "default"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "production",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "inputs": [
        "test",
        "^production"
      ],
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "options": {
        "retryFailedTests": true,
        "maxRetries": 3,
        "flakyTestRetryOptions": {
          "enabled": true,
          "maxAttempts": 3,
          "retryDelay": 500
        }
      }
    },
    "lint": {
      "inputs": [
        "lint",
        "^production"
      ],
      "cache": true
    },
    "typecheck": {
      "inputs": [
        "typecheck",
        "^production"
      ],
      "cache": true
    },
    "e2e": {
      "inputs": [
        "default"
      ],
      "dependsOn": [
        "^build"
      ],
      "options": {
        "retryFailedTests": true,
        "maxRetries": 2,
        "flakyTestRetryOptions": {
          "enabled": true,
          "maxAttempts": 2,
          "retryDelay": 1000
        }
      }
    }
  },
  "nxCloudId": "68b56409c98235f690fdfefd",
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "maxParallel": 1,
        "useDependencyGraph": true,
        "cacheableOperations": [
          "build",
          "test",
          "lint",
          "typecheck"
        ],
        "distributedTaskExecution": true,
        "retryableOperations": [
          "test",
          "e2e",
          "build"
        ],
        "flakyTaskRetryOptions": {
          "maxRetries": 3,
          "enabled": true,
          "retryDelay": 1000,
          "retryDelayMultiplier": 2,
          "maxRetryDelay": 10000
        }
      }
    }
  },
  "parallel": 1,
  "cacheDirectory": ".nx/cache",
  "useInferencePlugins": false,
  "envFile": ".env"
}